{
    "name": "bright-labs/craft-salesforce",
    "description": "Sync Salesforce data.",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "devs@brightlabs.com.au",
        "issues": "https://github.com/brightlabscomau/salesforce/issues?state=open",
        "source": "https://github.com/brightlabscomau/salesforce",
        "docs": "https://github.com/brightlabscomau/salesforce",
        "rss": "https://github.com/brightlabscomau/salesforce/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "brightlabs\\craftsalesforce\\": "src/"
        }
    },
    "extra": {
        "handle": "salesforce",
        "name": "Salesforce",
        "developer": "Bright Labs",
        "documentationUrl": "https://github.com/brightlabscomau/salesforce",
        "class": "brightlabs\\craftsalesforce\\Salesforce"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
